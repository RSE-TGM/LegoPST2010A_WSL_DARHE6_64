	 *--------------------*/\
\
#ifdef XOPEN_CATALOG\
	setlocale(LC_ALL, "");\
#endif\
\
  	UxTopLevel = UxInitialize("$PJ_APP_CLASS_NAME", &argc, argv);\
\
