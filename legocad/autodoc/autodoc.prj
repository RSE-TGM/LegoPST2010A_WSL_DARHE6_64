EXECUTABLE	= $(LEGOCAD_BIN)/$PJ_EXECUTABLE\
MAIN		= $PJ_MAIN_SRC\
INTERFACES	= $PJ_INTERFACES_SRC\
APPL_OBJS	= UxXt.o aux_autodoc.o unita.o note.o document.o doc_dati.o \\
		  doc_var.o\
UX_DIR		= \
UX_LIBPATH	= $(UX_DIR)/lib\
X_LIBS		= -lXm -lXt -lX11\
\
X_LIBPATH	= \
MOTIF_LIBPATH	=\
X_CFLAGS	=\
MOTIF_CFLAGS	=\
\
CFLAGS		= -D$(OS) -D_BSD -D_NO_PROTO -DXOPEN_CATALOG -DAIXV3 \\
		  $(X_CFLAGS) $(MOTIF_CFLAGS) -I$(LEGOROOT_INCLUDE) \\
                   -I$(LEGOCAD_INCLUDE) -g\
LIBPATH		= $(X_LIBPATH) $(MOTIF_LIBPATH) -L$(LEGOROOT_LIB)\
LIBS		= $(LEGOCAD_LIB)/liblegocad.a $(X_LIBS) -lm -lutilx -lutil\
\
\
OBJS = $(MAIN:.c=.o) $(INTERFACES:.c=.o) $(APPL_OBJS)\
\
\
.c.o:\
	$(CC) -c $(CFLAGS) $< -o $@\
\
\
$(EXECUTABLE): $(OBJS)\
	$(CC) $(OBJS) $(LIBPATH) $(LIBS) -o $(EXECUTABLE)\
	@echo "done"\
\
#\
# get all file from SCCS \
#\
get:\
	@echo\
	@echo --- AUTODOC estrazione sorgenti da SCCS ---\
	@echo\
	sccs get $(MAIN) \\
                 $(INTERFACES) \\
                 $(APPL_OBJS:.o=.c)\
	@echo\
	@echo --- done ---\
	@echo\
#\
# end of makefile\

*xtmake.PjEdited: 1
