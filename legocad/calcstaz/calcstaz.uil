object
	MAIN_WINDOW : XmMainWindow 
	{
	  arguments {
		      XmNx =  kx_main;
		      XmNy =  ky_main;
		      XmNunitType = Xm100TH_FONT_UNITS;
		    };
	   controls {
		      XmForm	 contenitore;
		      XmMenuBar      menubar;
		    };
	};


!****************************************************************************
!	Definizione Form e suoi controlli
!
object
	contenitore  : XmForm
	{
	   arguments {
		       XmNhorizontalSpacing = k_horizontal_offset + 5;
		       XmNverticalSpacing   = k_vertical_offset + 5;
		       XmNwidth  = kw_main;
		       XmNheight = kh_main - kh_menubar;
		      };
	     controls {
		       XmLabel		model_name;	
		       XmLabel		   model_name_val_iniz;
		       XmLabel		computing_mode;
!	DISABILITATA...
!       XmLabel		diagnostics_mode;
		       XmLabel		tolerance_label;
		       XmLabel		   tolerance_val_iniz;
		       XmFrame		status_window_frame;  
		       XmScrolledList	status_window;
		       XmPushButton 	m_start_button;
		       XmPushButton 	m_step_button;
        	      };
	};

