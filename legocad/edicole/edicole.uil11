object
	main_window : XmMainWindow 
	{
	  arguments {
		      XmNunitType = Xm100TH_FONT_UNITS;
		    };
	   controls {
		      XmMenuBar      menubar;
		      XmForm	 contenitore;
		    };
	};


!****************************************************************************
!	Definizione MenuBar e suoi controlli
!
object
	menubar      : XmMenuBar
	{
	   arguments {
		       XmNheight      = kh_menubar;
		       XmNorientation = XmHORIZONTAL;
		       XmNspacing     = 60;          !distanza fra le voci
		      };
	     controls {
		       XmCascadeButton	file_menu_entry;
		       XmCascadeButton	edit_menu_entry;
		       XmCascadeButton	bitmap_menu_entry;
		       XmCascadeButton	icon_menu_entry;
		      };
	};

!----------------------------------------------------------------------------
!	Definizione del CascadeButton e relativo Pulldown : FILE
!
object
	file_menu_entry : XmCascadeButton
	{
	   arguments {
		       XmNlabelString = k_file_label_text;
		       XmNtraversalOn = true;
		       XmNmnemonic    = file;
		      };
	     controls {
		       XmPulldownMenu file_menu;
		      };
	};

object
	file_menu    : XmPulldownMenu
	{
	    controls {
		       XmPushButton  m_quit_button;
		      };
	};

! Quit
object
	m_quit_button : XmPushButton
	{
	   arguments {
		       XmNlabelString = k_quit_label_text;
		      };
	   callbacks {
	       MrmNcreateCallback  = procedure  create_proc(k_quit_program);
	       XmNactivateCallback = procedure  activate_proc(k_quit_program);
		      };
	};


