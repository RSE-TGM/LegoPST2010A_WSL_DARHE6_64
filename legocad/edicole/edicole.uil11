        icon_width : XmScale
        {
           arguments {
                       XmNleftAttachment = XmATTACH_POSITION;
                       XmNleftPosition = 15;
                       XmNleftOffset = 15;
		       XmNbottomAttachment  = XmATTACH_POSITION;
		       XmNbottomPosition  = 40;
		       XmNdecimalPoints = 0;
		       XmNmaximum = k_icon_max_dimension / k_stepping;
		       XmNorientation = XmHORIZONTAL;
		       XmNprocessingDirection = XmMAX_ON_RIGHT;
		       XmNscaleHeight = 20;
		       XmNscaleWidth = 200;
		       XmNshowValue = true;
                      };
           callbacks {
               MrmNcreateCallback  = procedure create_proc(k_icon_width);
	          XmNdragCallback  = procedure drag_proc(k_icon_width);
          XmNvalueChangedCallback  = procedure drag_proc(k_icon_width);
                     };
        };

