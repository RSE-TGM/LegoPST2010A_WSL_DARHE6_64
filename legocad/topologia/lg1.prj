INTERFACES	= $PJ_INTERFACES_SRC\
APPL_OBJS	= UxXt.o _subproc.o\\
                  init_modello.o read_moduli.o read_f01.o errore.o \\
                  del_blocchi.o write_f01.o lg1_init_arg.o add_block.o\\
                  graphics_interface.o gest_filtri.o verifica_file.o\
\
X_LIBS		= -lXm -lXt -lX11\
\
LEGO_INCLUDE    = -I$(LEGOROOT_INCLUDE) -I$(LEGOCAD_INCLUDE)\
LEGO_LIBPATH    = -L$(LEGOCAD_LIB) -L$(LEGOROOT_LIB)\
\
X_LIBPATH	= \
MOTIF_LIBPATH	=\
X_CFLAGS	=\
MOTIF_CFLAGS	=\
\
CFLAGS		=  $(C_FLAGS) \\
		  $(X_CFLAGS) $(MOTIF_CFLAGS)  $(LEGO_INCLUDE) \\
		   -DTOPOLOGIA \\
		  -DSPMD -DRUNTIME -Dibmr2 -I.\
LIBPATH		= $(X_LIBPATH) $(MOTIF_LIBPATH) $(LEGO_LIBPATH)\
LIBS		= -llegocad $(LEGOCAD_LIB)/macro_topol.a\\
                 -licofile -lutilx -lutil $(X_LIBS) $(STUB_LIBS) -lbsd -lm \
                  \
          \
\
OBJS = $(MAIN:.c=.o) $(INTERFACES:.c=.o) $(APPL_OBJS)\
\
\
.c.o:\
	$(CC) -c -g $(CFLAGS) $< -o $@\
\
\
$(EXECUTABLE): $(OBJS)\
	$(CC) $(LINKER_OPTIONS) $(OBJS) $(LIBPATH) $(LIBS) -o $(EXECUTABLE)\
	@echo "done"\

*xtmake.PjEdited: 1
