	value kh_draw0		: 20;

	value kx_draw1		: 10;
	value ky_draw1		: (ky_draw0+kh_draw0+5);
	value kw_draw1		: (720+180);
	value kh_draw1		: 300;

	value kx_ord1		: (kx_draw1+kw_draw1+20);
	value ky_ord1		: (ky_draw1);
	value kw_ord1		: (kw_main-kx_ord1-10);
	value kh_ord1		: kh_draw1;
	
