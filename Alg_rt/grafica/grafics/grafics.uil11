	value ky_mis1		: (ky_draw1+kh_draw1+kh_tim1);
	value kw_mis1		: kw_draw1-150;
	value kh_mis1		: 60;

	value kx_val1		: (kx_draw1+kw_mis1);
	value ky_val1		: (ky_draw1+kh_draw1+kh_tim1);
	value kw_val1		: 150;
	value kh_val1		: kh_mis1;

