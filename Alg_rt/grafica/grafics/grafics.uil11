object                                  ! The main window widget.
                                        ! This displays the current order
                                        ! as it is taken.
        S_MAIN_WINDOW : XmMainWindow {

            arguments {
                XmNx = kx_main;
                XmNy = ky_main;
                XmNwidth = kw_main;
                XmNheight =kh_main;
            };

            controls {                  ! S_MAIN_WINDOW has two children.
                XmMenuBar        s_menu_bar;
		XmForm		 contenitore;
            };
	};
!****************************************************************************
!       Definizione Form e suoi controlli
!
object
        contenitore  : XmForm
        {
           arguments {
                       XmNhorizontalSpacing = k_horizontal_offset;
                       XmNverticalSpacing   = k_vertical_offset;
                       XmNwidth  = kw_main;
                       XmNheight = kh_main;
                       XmNbackground = gray;
                      };
             controls {
                XmDrawingArea    draw0;
                XmDrawingArea    draw1;
                XmDrawingArea    mis1;
                XmDrawingArea    ord1;
                XmDrawingArea    tim1;
                XmDrawingArea   tempo;
                XmDrawingArea    val1;
			};
        callbacks {
            MrmNcreateCallback = procedure create_proc (k_form);
                };

	};
       

object
        draw0 : XmDrawingArea {

                arguments {
                XmNx = 0;
                XmNy = ky_draw0;
                XmNwidth = kw_draw0;
                XmNheight =kh_draw0;
                XmNbackground = blue;
               XmNborderWidth = 0;
               XmNmarginWidth = 0;
               XmNmarginHeight = 0;
		XmNrightAttachment= XmATTACH_FORM;
		XmNrightOffset=kw_ord1+5+k_horizontal_offset;
		XmNleftAttachment = XmATTACH_FORM;

                        };
        callbacks {
            XmNexposeCallback = procedure draw_proc (k_draw0);
            MrmNcreateCallback = procedure create_proc (k_draw0);
                };
        };
