INTERFACES	= $PJ_INTERFACES_SRC\
APPL_OBJS	= graphics.o\
UX_DIR		= /usr/lpp/aic\
UX_LIBPATH	= $(UX_DIR)/lib\
X_LIBS		= -lXm -lXt -lX11\
\
X_LIBPATH	= \
MOTIF_LIBPATH	=\
X_CFLAGS	=\
