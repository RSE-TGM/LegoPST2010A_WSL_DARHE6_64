\
EXECUTABLE	= $(LEGOMMI_BIN)/$PJ_EXECUTABLE\
MAIN		= $PJ_MAIN_SRC\
INTERFACES	= $PJ_INTERFACES_SRC\
APPL_OBJS	= UxXt.o graphics.o ColorViewer.o\\
                  ColorEditor.o\\
		  ColorMapViewer.o\\
		  UtilRes.o draw_wid.o draw_other.o\
\
UX_DIR		= /usr/lpp/aic\
UX_LIBPATH	= $(UX_DIR)/lib\
X_LIBS		= $(X_LIB)\
\
X_LIBPATH	= -L/usr/lib \
X_CFLAGS	= -I/usr/include\\
                        -I$(X_INCLUDE)\\
                        -I$(LEGORT_INCLUDE)\\
                        -I$(LEGORT_INCLUDE)/widget\\
                        -I$(LEGOMMI_INCLUDE)\\
                        -I$(LEGOMMI_LIB)\\
                        -I$(UX_DIR)/include\
\
APPL_INCLUDE    =  -I$(LEGOCAD_INCLUDE) -I$(LEGOROOT_INCLUDE) -I$(X_INCLUDE) \\
                  -I$(LEGOMMI_INCLUDE) -I$(LEGORT_INCLUDE) -I$(LEGOMMI_LIB)\
\
CFLAGS		= -DXLIB_ILLEGAL_ACCESS $(VERSIONE) $(C_FLAGS)\\
		  $(X_CFLAGS) $(APPL_INCLUDE)\
\
LIBPATH		= -L$(LEGOROOT_LIB)\\
		$(X_LIBPATH)\\
		-L$(LEGOMMI_LIB) \
\
LIBS		= 	$(LEGOMMI_LIB)/libXl.a\\
			$(LEGOMMI_LIB)/libOl.a\\
			$(LEGOMMI_LIB)/libCs.a\\
			$(LEGOMMI_LIB)/libXd.a\\
                        $(LEGORT_LIB)/libnet.a\\
                        $(LEGORT_LIB)/libsim.a\\
                        $(LEGORT_LIB)/libcom.a\\
                        $(LEGORT_LIB)/libipc.a\\
                        -lutil -lutilx $(X_LIBS) $(STUB_LIBS) -lm -lbsd\
\
LIBRERIE	= $(LEGOMMI_LIB)/libXl.a $(LEGOMMI_LIB)/libOl.a\\
			$(LEGOMMI_LIB)/libCs.a $(LEGOMMI_LIB)/libXd.a\
\
OBJS = $(MAIN:.c=.o) $(INTERFACES:.c=.o) $(APPL_OBJS)\
\
\
.c.o:\
	$(CC) -c $(CFLAGS) $< -o $@\
\
\
$(EXECUTABLE): $(OBJS) $(LIBRERIE)\
	$(CC) $(LINKER_OPTIONS) $(OBJS) $(LIBPATH) $(LIBS) -o $(EXECUTABLE)\
	@echo "done"\
#\
#   oggetti provenienti da altri direttori\
